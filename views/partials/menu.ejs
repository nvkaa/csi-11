        <div class="menu-item">
            <form action="/menu-redirect" method="post">
                <a href="#menu" class="order-a" home>
                    <input type="submit" value="Menu">
                </a>
            </form>
            
            <form action="/orders-redirect" method="post">
                <a class="order-a">
                    <input type="submit" value="Orders">
                </a>
