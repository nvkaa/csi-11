        </div>
    <% } else { %>
        <div class="menu-item">

            <form action="/home-redirect" method="post">
                <a class="order-a" home>
                    <input type="submit" value="Home">
                </a>
            </form>

            <form action="/order-history-redirect" method="post">
                <a class="order-a">
                    <input type="submit" value="Orders">
                </a>
